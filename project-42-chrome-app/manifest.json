{
    "manifest_version": 2,
    "name": "Save Sick Child App Photo Uploading app",
    "version": "1",
    "app": {
        "background": {
            "scripts": ["main.js"]
        }
    }, "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
},
    "permissions": [
        "webview",
        "videoCapture",
        "experimental", {
            "socket": ["tcp-connect", "tcp-listen"]
        }]
}